        "bazelbsp.bsp.autoUpdateOnTargetChange": {
          "type": "boolean",
          "default": true,
          "description": "Automatically prompt to update BSP configuration when selecting a new Bazel target. Set to false to disable the prompt."
        },
        "bazelbsp.bazel.buildMode": {
          "type": "string",
          "default": "ask",
          "enum": [
            "debug",
            "release",
            "release-with-symbols",
            "ask"
          ],
          "enumDescriptions": [
            "Always build debug: unoptimized with symbols (--compilation_mode=dbg --copt=-g --strip=never)",
            "Always build release: optimized without symbols (default Bazel settings)",
            "Always build release with symbols: optimized with symbols for crash reports (--compilation_mode=opt --copt=-g --strip=never)",
            "Ask every time (default) - remembers last selection"
          ],
          "description": "Default build mode for Bazel Build command. Options: 'debug' (unoptimized+symbols), 'release' (optimized), 'release-with-symbols' (optimized+symbols for crash reports), or 'ask' (prompts each time, remembers choice)."
        },
        "bazelbsp.system.taskExecutor": {
          "type": "string",
          "default": "v2",
          "enum": [
            "v1",
            "v2"
          ],
          "enumDescriptions": [
            "Legacy executor, use this if you have issues with the new executor.",
            "New executor, use this for better performance and better error handling."
          ],
          "description": "Use version task executor for build tasks."
        },
        "bazelbsp.system.logLevel": {
          "type": "string",
          "default": "info",
          "enum": [
            "debug",
            "info",
            "warn",
            "error"
          ],
          "description": "Log level for bazelbsp."
        },
        "bazelbsp.system.autoRevealTerminal": {
          "type": "boolean",
          "default": true,
          "description": "Automatically reveal the terminal when executing a command or task"
        },
        "bazelbsp.system.showProgressStatusBar": {
          "type": "boolean",
          "default": true,
          "description": "Show progress in the status bar when executing a command or task"
        },
        "bazelbsp.xcodebuildserver.autogenerate": {
          "type": "boolean",
          "default": false,
          "description": "Watch if default scheme is changed and regenerate the build server config"
        },
        "bazelbsp.bazel.queryExcludePaths": {
          "type": "array",
          "items": {
            "type": "string"
