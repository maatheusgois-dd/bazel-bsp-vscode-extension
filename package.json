{
  "name": "bazel-bsp-vscode-extension",
  "type": "commonjs",
  "displayName": "Bazel BSP",
  "description": "VSCode extension for Bazel-based development with BSP integration",
  "version": "0.1.47",
  "publisher": "maatheusgois-dd",
  "icon": "images/logo.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maatheusgois-dd/bazel-bsp-vscode-extension.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Formatters",
    "Linters",
    "Extension Packs",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onCommand:workbench.action.tasks.runTask",
    "workspaceContains:**/BUILD",
    "workspaceContains:**/WORKSPACE",
    "workspaceContains:**/BUILD.bazel",
    "workspaceContains:**/WORKSPACE.bazel",
    "workspaceContains:**/BUILD.bazel.lock",
    "workspaceContains:**/WORKSPACE.bazel.lock",
    "workspaceContains:**/BUILD.bazel.lock",
    "onLanguage:swift",
    "onDebug"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "taskDefinitions": [
      {
        "type": "bazelbsp",
        "required": [
          "action"
        ],
        "properties": {
          "action": {
            "type": "string",
            "enum": [
              "launch",
              "build",
              "run",
              "clean",
              "test",
              "resolve-dependencies",
              "debugging-launch",
              "debugging-build",
              "debugging-run"
            ]
          },
          "scheme": {
            "type": "string",
            "description": "Scheme to build",
            "examples": [
              "MyApp",
              "MyAppTests"
            ]
          },
          "configuration": {
            "type": "string",
            "description": "Configuration to build",
            "examples": [
              "Debug",
              "Release"
            ]
          },
          "workspace": {
            "type": "string",
            "description": "Path to workspace for resolving dependencies",
            "examples": [
              "./MyApp.xcworkspace",
              "/Users/user/Developer/MyApp/MyApp.xcworkspace"
            ]
          },
          "simulator": {
            "type": "string",
            "description": "Simulator to launch. Currently only device UUID is supported. Deprecated: use `destinationId` instead",
            "examples": [
              "00000000-0000-0000-0000-000000000000"
            ]
          },
          "destinationId": {
            "type": "string",
            "description": "Device ID to launch the app on",
            "examples": [
              "00000000-0000-0000-0000-000000000000"
            ]
          },
          "destination": {
            "type": "string",
            "description": "Raw destination string to launch the app on. Alternative to `destinationId`",
            "examples": [
              "platform=iOS Simulator,name=iPhone 12,OS=15.0",
              "generic/platform=iOS Simulator",
              "platform=iOS Simulator,id=00000000-0000-0000-0000-000000000000"
            ]
          },
          "launchArgs": {
            "type": "array",
            "description": "Arguments to pass to the app on launch",
            "items": {
              "type": "string"
            },
            "examples": [
              [
                "--my-arg",
                "value"
              ]
            ]
          },
          "launchEnv": {
            "type": "object",
            "description": "Environment variables to pass to the app on launch",
            "examples": [
              {
                "MY_ENV": "value"
              }
            ]
          }
        }
      }
    ],
    "problemMatchers": [
      {
        "name": "bazelbsp-watch",
        "label": "Bazel BSP (watch)",
        "owner": "bazelbsp",
        "fileLocation": [
          "relative",
          "${workspaceRoot}"
        ],
        "pattern": [
          {
            "regexp": "^✘ \\[ERROR\\] (.+)$",
            "message": 1
          },
          {
            "regexp": "^    (.+):(\\d+):(\\d+):$",
            "file": 1,
            "line": 2,
            "column": 3
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^\\[watch\\] build started",
          "endsPattern": "^\\[watch\\] build finished"
        }
      },
      {
        "name": "bazelbsp-xcodebuild-default",
        "owner": "bazelbsp",
        "label": "Bazel BSP xcodebuild (default)",
        "fileLocation": [
          "absolute"
        ],
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(error|warning):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
          }
        ]
      },
      {
        "name": "bazelbsp-xcbeautify-errors",
        "owner": "bazelbsp",
        "label": "Bazel BSP xcbeautify (errors)",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^(?:❌|\\[x\\])\\s*(\\/[^:]+):(\\d+):(\\d+):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        },
        "severity": "error"
      },
      {
        "name": "bazelbsp-xcbeautify-warnings",
        "owner": "bazelbsp",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^(?:⚠️|\\[!\\])\\s*(\\/[^:]+):(\\d+):(\\d+):\\s*(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        },
        "severity": "warning"
      }
    ],
    "debuggers": [
      {
        "type": "bazelbsp-lldb",
        "label": "Bazel BSP (LLDB)",
        "languages": [
          "swift",
          "objective-c",
          "objective-cpp"
        ],
        "configurationSnippets": [
          {
            "label": "Bazel BSP: Build and Run (Wait for debugger)",
            "body": {
              "type": "bazelbsp-lldb",
              "request": "attach",
              "name": "Bazel BSP: Build and Run (Wait for debugger)",
              "preLaunchTask": "bazelbsp: debugging-launch"
            }
          }
        ],
        "configurationAttributes": {
          "attach": {
            "description": "Attach to running app",
            "required": [],
            "type": "object",
            "properties": {
              "codelldbAttributes": {
                "title": "CodeLLDB attributes",
                "description": "Attributes to pass to CodeLLDB. See: https://github.com/vadimcn/codelldb/blob/v1.10.0/MANUAL.md",
                "type": "object",
                "examples": [
                  {
                    "initCommands": [
                      "script print('Hello, bazelbsp!')"
                    ]
                  }
                ]
              },
              "continueOnAttach": {
                "type": "boolean",
                "default": true,
                "description": "Continue the app after attaching. If false, the app will be paused after attaching."
              }
            }
          },
          "launch": {
            "description": "Deprecated: use `attach` instead",
            "required": [],
            "type": "object",
            "properties": {
              "codelldbAttributes": {
                "title": "CodeLLDB attributes",
                "description": "Attributes to pass to CodeLLDB. See: https://github.com/vadimcn/codelldb/blob/v1.10.0/MANUAL.md",
                "type": "object",
                "examples": [
                  {
                    "initCommands": [
                      "script print('Hello, bazelbsp!')"
                    ]
                  }
                ]
              }
            }
          }
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bazelbsp-view",
          "title": "Bazel BSP",
          "icon": "images/logo-panel.png",
          "when": "bazelbsp.enabled"
        }
      ]
    },
    "views": {
      "bazelbsp-view": [
        {
          "id": "bazelbsp.view.bazelQuery",
          "name": "BAZEL TARGETS",
          "visibility": "visible"
        },
        {
          "id": "bazelbsp.destinations.view",
          "name": "DESTINATIONS",
          "visibility": "collapsed"
        },
        {
          "id": "bazelbsp.tools.view",
          "name": "TOOLS",
          "visibility": "collapsed"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "bazelbsp.build.view",
        "contents": "No Bazel targets found.\n\nMake sure your workspace contains BUILD or BUILD.bazel files.\n\n[Refresh Targets](command:bazelbsp.bazelQuery.refresh)\n[Run Diagnose](command:bazelbsp.build.diagnoseSetup)",
        "when": "bazelbsp.build.noSchemes"
      },
      {
        "view": "bazelbsp.devices.view",
        "contents": "No devicectl found. Install the latest version of Xcode to manage device.",
        "when": "bazelbsp.devices.noDevicectl"
      },
      {
        "view": "bazelbsp.devices.view",
        "contents": "No devices found. Connect an iOS device via USB or Wi-Fi to get started.",
        "when": "bazelbsp.devices.noDevices"
      }
    ],
    "commands": [
      {
        "command": "bazelbsp.build.launch",
        "title": "Bazel BSP: Build & Run (Launch)",
        "icon": "$(bazelbsp-player-play)"
      },
      {
        "command": "bazelbsp.build.debugLaunch",
        "title": "Bazel BSP: Build & Debug",
        "icon": "$(debug-alt)",
        "description": "Build and launch with debugger"
      },
      {
        "command": "bazelbsp.build.run",
        "title": "Bazel BSP: Run (without Build)",
        "icon": "$(play)"
      },
      {
        "command": "bazelbsp.build.build",
        "title": "Bazel BSP: Build (without Run)",
        "icon": "$(bazelbsp-tool)"
      },
      {
        "command": "bazelbsp.build.clean",
        "title": "Bazel BSP: Clean",
        "icon": "$(clear-all)"
      },
      {
        "command": "bazelbsp.build.test",
        "title": "Bazel BSP: Test",
        "icon": "$(beaker)"
      },
      {
        "command": "bazelbsp.build.testWithSwiftTesting",
        "title": "Bazel BSP: Test with Swift Testing",
        "icon": "$(beaker)",
        "description": "Run tests using Swift Testing framework"
      },
      {
        "command": "bazelbsp.build.resolveDependencies",
        "title": "Bazel BSP: Resolve dependencies",
        "icon": "$(sync)"
      },
      {
        "command": "bazelbsp.build.removeBundleDir",
        "title": "Bazel BSP: Remove bundle directory",
        "icon": "$(bazelbsp-trash-x)"
      },
      {
        "command": "bazelbsp.build.generateBuildServerConfig",
        "title": "Bazel BSP: Generate Build Server Config (buildServer.json)",
        "icon": "$(file-code)"
      },
      {
        "command": "bazelbsp.build.selectXcodeWorkspace",
        "title": "Bazel BSP: Select",
        "icon": "$(file-directory)"
      },
      {
        "command": "bazelbsp.build.setDefaultScheme",
        "title": "Bazel BSP: Set scheme",
        "icon": "$(target)"
      },
      {
        "command": "bazelbsp.build.diagnoseSetup",
        "title": "Bazel BSP: Diagnose build setup",
        "icon": "$(bug)"
      },
      {
        "command": "bazelbsp.bazel.build",
        "title": "Bazel BSP: Bazel Build",
        "icon": "$(bazelbsp-tool)",
        "description": "Build Bazel target"
      },
      {
        "command": "bazelbsp.bazel.test",
        "title": "Bazel BSP: Bazel Test",
        "icon": "$(beaker)",
        "description": "Test Bazel target"
      },
      {
        "command": "bazelbsp.bazel.selectTarget",
        "title": "Bazel BSP: Select Bazel Target",
        "icon": "$(target)",
        "description": "Select Bazel target for build/test commands"
      },
      {
        "command": "bazelbsp.bazel.selectBuildMode",
        "title": "Bazel BSP: Select Build Mode",
        "icon": "$(settings-gear)",
        "description": "Select build mode (Debug or Release)"
      },
      {
        "command": "bazelbsp.bazel.clean",
        "title": "Bazel BSP: Bazel Clean",
        "icon": "$(trash)",
        "description": "Clean Bazel build cache"
      },
      {
        "command": "bazelbsp.bazel.cleanExpunge",
        "title": "Bazel BSP: Bazel Clean (Expunge)",
        "icon": "$(trash-x)",
        "description": "Clean Bazel build cache with expunge (complete cleanup)"
      },
      {
        "command": "bazelbsp.bazel.buildSelected",
        "title": "Bazel BSP: Build Selected Bazel Target",
        "icon": "$(bazelbsp-tool)",
        "description": "Build currently selected Bazel target"
      },
      {
        "command": "bazelbsp.bazel.testSelected",
        "title": "Bazel BSP: Test Selected Bazel Target",
        "icon": "$(beaker)",
        "description": "Test currently selected Bazel target"
      },
      {
        "command": "bazelbsp.bazel.run",
        "title": "Bazel BSP: Bazel Run",
        "icon": "$(run)",
        "description": "Run Bazel target on iOS simulator"
      },
      {
        "command": "bazelbsp.bazel.debug",
        "title": "Bazel BSP: Bazel Debug",
        "icon": "$(debug-alt)",
        "description": "Debug Bazel target on iOS simulator or device"
      },
      {
        "command": "bazelbsp.bazel.stop",
        "title": "Bazel BSP: Stop/Cancel",
        "icon": "$(debug-stop)",
        "description": "Stop debugging or cancel running build/test"
      },
      {
        "command": "bazelbsp.bazel.runSelected",
        "title": "Bazel BSP: Run Selected Bazel Target",
        "icon": "$(run)",
        "description": "Run currently selected Bazel target on iOS simulator"
      },
      {
        "command": "bazelbsp.bazelQuery.refresh",
        "title": "Bazel BSP: Refresh Bazel Targets",
        "icon": "$(refresh)",
        "description": "Refresh bazel query results"
      },
      {
        "command": "bazelbsp.bazelQuery.clearRecents",
        "title": "Bazel BSP: Clear Recent Targets",
        "icon": "$(clear-all)",
        "description": "Clear recent targets list"
      },
      {
        "command": "bazelbsp.build.refreshView",
        "title": "Bazel BSP: Refresh view",
        "icon": "$(bazelbsp-refresh)"
      },
      {
        "command": "bazelbsp.simulators.refresh",
        "title": "Bazel BSP: Refresh simulators list",
        "icon": "$(bazelbsp-refresh)"
      },
      {
        "command": "bazelbsp.simulators.openSimulator",
        "title": "Bazel BSP: Open simulator",
        "icon": "$(bazelbsp-device-mobile-heart)"
      },
      {
        "command": "bazelbsp.simulators.removeCache",
        "title": "Bazel BSP: Remove simulator cache",
        "icon": "$(bazelbsp-trash-x)"
      },
      {
        "command": "bazelbsp.simulators.start",
        "title": "Bazel BSP: Start simulator",
        "icon": "$(debug-start)"
      },
      {
        "command": "bazelbsp.simulators.stop",
        "title": "Bazel BSP: Stop simulator",
        "icon": "$(debug-stop)"
      },
      {
        "command": "bazelbsp.devices.refresh",
        "title": "Bazel BSP: Refresh devices list",
        "icon": "$(bazelbsp-refresh)"
      },
      {
        "command": "bazelbsp.tools.install",
        "title": "Bazel BSP: Install tool",
        "icon": "$(bazelbsp-terminal-2)"
      },
      {
        "command": "bazelbsp.tools.documentation",
        "title": "Bazel BSP: Open tool documentation",
        "icon": "$(bazelbsp-book)"
      },
      {
        "command": "bazelbsp.tools.refresh",
        "title": "Bazel BSP: Refresh tools list",
        "icon": "$(bazelbsp-refresh)",
        "enablement": "view == bazelbsp.tools.view"
      },
      {
        "command": "bazelbsp.system.resetbazelbspCache",
        "title": "Bazel BSP: Reset Extension Cache",
        "icon": "$(clear-all)"
      },
      {
        "command": "bazelbsp.system.testErrorReporting",
        "title": "Bazel BSP: Test Error Reporting",
        "icon": "$(bug)"
      },
      {
        "command": "bazelbsp.system.openTerminalPanel",
        "title": "Bazel BSP: Open Terminal Panel",
        "icon": "$(terminal)"
      },
      {
        "command": "bazelbsp.system.setupSwiftExtension",
        "title": "Bazel BSP: Setup Swift Extension for BSP",
        "icon": "$(settings-gear)",
        "description": "Configure Swift extension settings for selected Bazel target"
      },
      {
        "command": "bazelbsp.system.showSwiftConfigStatus",
        "title": "Bazel BSP: Show Swift Configuration Status",
        "icon": "$(info)",
        "description": "Check Swift extension configuration"
      },
      {
        "command": "bazelbsp.system.setupBSPConfig",
        "title": "Bazel BSP: Setup BSP Config for Selected Target",
        "icon": "$(server-process)",
        "description": "Generate .bsp/skbsp.json configuration for Build Server Protocol"
      },
      {
        "command": "bazelbsp.system.monitorBSPLogs",
        "title": "Bazel BSP: Monitor BSP Logs",
        "icon": "$(debug-console)",
        "description": "Stream sourcekit-bazel-bsp logs in real-time for debugging"
      },
      {
        "command": "bazelbsp.debugger.getAppPath",
        "title": "Bazel BSP: Get app path for debugging",
        "icon": "$(file-code)"
      },
      {
        "command": "bazelbsp.destinations.select",
        "title": "Bazel BSP: Select destination",
        "icon": "$(bazelbsp-pin)"
      },
      {
        "command": "bazelbsp.destinations.removeRecent",
        "title": "Bazel BSP: Remove recent destination",
        "icon": "$(bazelbsp-trash-x)"
      },
      {
        "command": "bazelbsp.destinations.selectForTesting",
        "title": "bazelbsp.Testing: Select destination for testing",
        "icon": "$(file-code)"
      },
      {
        "command": "bazelbsp.simulators.screenshot",
        "title": "Bazel BSP: Take Simulator Screenshot",
        "icon": "$(camera)"
      }
    ],
    "keybindings": [
      {
        "command": "bazelbsp.bazel.runSelected",
        "key": "cmd+r",
        "mac": "cmd+r"
      },
      {
        "command": "bazelbsp.bazel.buildSelected",
        "key": "cmd+b",
        "mac": "cmd+b"
      },
      {
        "command": "bazelbsp.bazel.testSelected",
        "key": "cmd+u",
        "mac": "cmd+u"
      },
      {
        "command": "bazelbsp.bazel.clean",
        "key": "cmd+k",
        "mac": "cmd+k"
      },
      {
        "command": "bazelbsp.bazel.cleanExpunge",
        "key": "cmd+shift+k",
        "mac": "cmd+shift+k"
      }
    ],
    "menus": {
      "commandPalette": [],
      "view/title": [
        {
          "command": "bazelbsp.bazelQuery.refresh",
          "when": "view == bazelbsp.view.bazelQuery",
          "group": "navigation@1"
        },
        {
          "command": "bazelbsp.build.removeBundleDir",
          "when": "view == bazelbsp.build.view",
          "group": "navigation@1"
        },
        {
          "command": "bazelbsp.tools.refresh",
          "when": "view == bazelbsp.tools.view",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "bazelbsp.simulators.openSimulator",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-group-simulator-.*/",
          "group": "inline@3"
        },
        {
          "command": "bazelbsp.simulators.refresh",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-group-simulator-.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.simulators.removeCache",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-group-simulator-.*/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.devices.refresh",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-group-device-.*/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.simulators.start",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-simulator.*&status=shutdown.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.simulators.stop",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-simulator.*&status=booted.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.simulators.screenshot",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-simulator.*&status=booted.*/",
          "group": "context@1"
        },
        {
          "command": "bazelbsp.destinations.select",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-.*/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.destinations.select",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-.*/",
          "group": "context@1"
        },
        {
          "command": "bazelbsp.destinations.removeRecent",
          "when": "view == bazelbsp.destinations.view && viewItem =~ /destination-item-.*/ && viewItem =~ /.*recent=true.*/",
          "group": "context@2"
        },
        {
          "command": "bazelbsp.tools.documentation",
          "when": "view == bazelbsp.tools.view",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.tools.install",
          "when": "view == bazelbsp.tools.view && viewItem == notInstalled",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.build.launch",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "inline@3"
        },
        {
          "command": "bazelbsp.build.debugLaunch",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "inline@4"
        },
        {
          "command": "bazelbsp.build.build",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.build.setDefaultScheme",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.build.launch",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@1"
        },
        {
          "command": "bazelbsp.build.build",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@2"
        },
        {
          "command": "bazelbsp.build.run",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@3"
        },
        {
          "command": "bazelbsp.build.debugLaunch",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@3.5"
        },
        {
          "command": "bazelbsp.build.test",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@4"
        },
        {
          "command": "bazelbsp.build.testWithSwiftTesting",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@5"
        },
        {
          "command": "bazelbsp.build.clean",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@6"
        },
        {
          "command": "bazelbsp.build.resolveDependencies",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@7"
        },
        {
          "command": "bazelbsp.build.setDefaultScheme",
          "when": "viewItem == bazelbsp.build.view.item",
          "group": "context@8"
        },
        {
          "command": "bazelbsp.bazel.selectTarget",
          "when": "viewItem =~ /bazelbsp\\.bazel\\.target/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.bazel.build",
          "when": "viewItem =~ /^bazelbsp\\.bazel\\.target\\.(library|binary).*&selected=true.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.bazel.test",
          "when": "viewItem =~ /^bazelbsp\\.bazel\\.target\\.test.*&selected=true.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.bazel.run",
          "when": "viewItem =~ /^bazelbsp\\.bazel\\.target\\.binary.*&selected=true.*/",
          "group": "inline@3"
        },
        {
          "command": "bazelbsp.bazel.debug",
          "when": "viewItem =~ /^bazelbsp\\.bazel\\.target\\.binary.*&selected=true.*/",
          "group": "inline@4"
        },
        {
          "command": "bazelbsp.bazel.stop",
          "when": "viewItem =~ /^bazelbsp\\.bazel\\.target\\.binary.*&selected=true.*/",
          "group": "inline@5"
        },
        {
          "command": "bazelbsp.bazel.selectTarget",
          "when": "viewItem =~ /bazelbsp\\.bazel\\.target/",
          "group": "context@1"
        },
        {
          "command": "bazelbsp.bazel.build",
          "when": "viewItem =~ /bazelbsp\\.bazel\\.target/",
          "group": "context@2"
        },
        {
          "command": "bazelbsp.bazel.test",
          "when": "viewItem == bazelbsp.bazel.target.test",
          "group": "context@3"
        },
        {
          "command": "bazelbsp.bazel.run",
          "when": "viewItem == bazelbsp.bazel.target.binary",
          "group": "context@4"
        },
        {
          "command": "bazelbsp.bazel.debug",
          "when": "viewItem == bazelbsp.bazel.target.binary",
          "group": "context@5"
        },
        {
          "command": "bazelbsp.bazel.build",
          "when": "viewItem =~ /^bazelTarget-(buildable|runnable).*&selected=true.*/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.bazel.test",
          "when": "viewItem =~ /^bazelTarget-test.*&selected=true.*/",
          "group": "inline@1"
        },
        {
          "command": "bazelbsp.bazel.run",
          "when": "viewItem =~ /^bazelTarget-runnable.*&selected=true.*/",
          "group": "inline@2"
        },
        {
          "command": "bazelbsp.bazel.debug",
          "when": "viewItem =~ /^bazelTarget-runnable.*&selected=true.*/",
          "group": "inline@3"
        },
        {
          "command": "bazelbsp.bazel.stop",
          "when": "viewItem =~ /^bazelTarget-runnable.*&selected=true.*/",
          "group": "inline@4"
        },
        {
          "command": "bazelbsp.bazelQuery.clearRecents",
          "when": "viewItem == bazelQueryRecents",
          "group": "inline@1"
        }
      ]
    },
